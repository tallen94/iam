import json
import requests

args = input()
data = json.loads(args)
endpoints = data[0]["endpoints"]
pool = data[1]

results = []
for endpoint in endpoints:
    res = requests.post("http://" + endpoint + "/executable/" + pool["username"] + "/pool/" + pool["name"] + "/run")
    results.append(res.text)

out=results
print(json.dumps(out))
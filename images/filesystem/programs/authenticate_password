import json
import hashlib
import base64
import hashlib, binascii

args = raw_input()
data = json.loads(args)
user = data[0][0]
user_password = user["password"]
given_password = data[1]["password"]

if user_password == given_password:
  out = json.dumps({"user": {"id": user["id"], "username": user["username"]}})
else:
  out = json.dumps({"error": "failed to authenticate"})
  
print out

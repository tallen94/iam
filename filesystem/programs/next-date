import croniter
import datetime
import calendar
import json
###

args = raw_input()
data = json.loads(args)

now = datetime.datetime.utcnow()
sched = data["cronExpr"]

cron = croniter.croniter(sched, now)
nextdate = cron.get_next(datetime.datetime)
# 2019-06-03 21:25:00
datetime1 = datetime.datetime.strptime(str(nextdate), "%Y-%m-%d %H:%M:%S")
timeInSeconds = calendar.timegm(datetime1.utctimetuple())
print timeInSeconds